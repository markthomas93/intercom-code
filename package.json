{
  "name": "intercom-code",
  "version": "0.1.0",
  "description": "A chrome extension for writing code snippets with Intercom",
  "scripts": {
    "clean": "trash dist",
    "build": "webpack --mode production ./src/content/index.js --output ./dist/content.js",
    "rebuild": "npm run clean && npm run build",
    "develop": "webpack --mode development --watch ./src/content/index.js --output ./dist/content.js"
  },
  "author": "Saad Quadri",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "copy-webpack-plugin": "^4.5.1",
    "flow-bin": "^0.67.1",
    "flow-interfaces-chrome": "^0.4.0",
    "trash-cli": "^1.4.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11"
  }
}
